{
  "name": "rnmarket-backend",
  "version": "1.0.0",
  "description": "Node/Express backend for RNmarket",
  "main": "index.ts",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "start": "ts-node ./src/app.ts",
    "startprod": "NODE_ENV=production node ./build/src/app.js",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --files ./src/app.ts",
    "test-node": "NODE_ENV=test-node jest --runInBand",
    "test-docker": "NODE_ENV=test-docker jest --runInBand",
    "lint": "eslint .",
    "prettier": "prettier . '!./postgresql' --write",
    "tsc": "tsc",
    "build": "npm run tsc && tsc-alias -p tsconfig.json && cp -r data/* build/data"
  },
  "author": "Jimi Pitk√§niitty",
  "license": "UNLICENSED",
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.9",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3",
    "umzug": "^3.8.1",
    "uuid": "^10.0.0",
    "tsc-alias": "^1.8.11"
  }
}
